<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="Modelo.Novedad" table="NOVEDADES">
        <id name="idnovedades" column="IDNOVEDADES">
            <generator class="identity" />
        </id>
        <property name="fechainicio" type="date" column="FECHAINICIO" />
        <property name="fechafin" type="date" column="FECHAFIN" />
        <property name="observaciones" type="string" column="OBSERVACIONES" />
        <many-to-one name="oempleado" 
        column="IDEMPLEADOS"
        not-null="true"/>
    </class>
</hibernate-mapping>
