<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="Modelo.Registro" table="REGISTROS">
        <id name="idregistros" column="IDREGISTROS">
            <generator class="identity" />
        </id>
        <property name="registro" type="date" column="REGISTRO" />
        <property name="foto" type="string" column="FOTO" />
        <many-to-one name="empleado" column="idempleados" />
    </class>
</hibernate-mapping>
